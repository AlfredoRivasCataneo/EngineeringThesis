@article{Gargiulo2009,
abstract = {Robotic operations are one of the major maintenance challenges for ITER and future fusion reactors. CEA has developed a multipurpose carrier able to realize deployments in the plasma vessel without breaking the Ultra High Vacuum (UHV) and temperature conditioning. A 6 years R\&D programme was jointly conducted by CEA-LIST Interactive Robotics Unit and the Institute for Magnetic Fusion Research (IRFM) in order to demonstrate the feasibility and reliability of an in-vessel inspection robot relevant to ITER requirements. The Articulated Inspection Arm robot (AIA) is an 8-m long multilink carrier with a payload up to 10 kg operable between plasma under tokamak conditioning environment; its geometry allows a complete close inspection of Plasma Facing Components (PFCs) of the Tore Supra vessel. Different tools are being developed by CEA to be plugged at the front head of the carrier. The diagnostic presently in operation consists in a viewing system offering accurate visual inspection of PFCs. Leak detection of first wall based on helium sniffing and laser compact system for carbon co-deposited layers characterizations or treatments are also considered for demonstration. In April 2008, the AIA robot equipped with its vision diagnostic has realized a complete deployment into Tore Supra and the first closed inspection of the vessel under UHV conditions. During the upcoming experimental campaign, the same operation will be performed under relevant conditions (10-6 Pa and 120 °C) after a conditioning phase at 200 °C to avoid outgassing pollution of the chamber. This paper describes the different steps of the project development, robot capabilities with the present operations conducted on Tore Supra and future requirements for making the robot a tool for tokamak routine operation.},
author = {Gargiulo, Laurent and Bayetti, Pascal and Bruno, Vincent and Hatchressian, Jean-Claude and Hernandez, Caroline and Houry, Michael and Keller, Delphine and Martins, Jean-Pierre and Measson, Yvan and Perrot, Yann},
doi = {10.1016/j.fusengdes.2008.11.043},
issn = {09203796},
journal = {Fusion Engineering and Design},
keywords = {aia,articulated inspection arm,diagnostic,fusion,iter,laser ablation,maintenance,remote handling,robot,tokamak,tore supra,vision},
month = jun,
number = {2-6},
pages = {220--223},
title = {{Operation of an ITER relevant inspection robot on Tore Supra tokamak}},
url = {http://dx.doi.org/10.1016/j.fusengdes.2008.11.043},
volume = {84},
year = {2009}
}
